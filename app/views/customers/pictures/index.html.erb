<% content_for :title, "Pictures" %>

<div class="container">
  <div class="row">
    <% @pictures.each do |picture| %>
    <div class="col-md-6">
      <div class="col-centered">
        <%= cl_image_tag picture.file , :height=>500, :width=>500, :crop=>"fill" %>
      </div>
    </div>
    <% end %>
  </div>
</div>
